{
  "name": "teacher-bo",
  "author": "dongzoolee",
  "private": true,
  "main": "index",
  "version": "0.0.1",
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=18.0.0",
    "yarn": "1.22.22"
  },
  "scripts": {
    "start": "RCT_METRO_PORT=1001 expo start --port 1001",
    "start:tunnel": "expo start --tunnel",
    "prebuild": "expo prebuild --yarn",
    "android": "RCT_METRO_PORT=1001 expo run:android",
    "ios": "RCT_METRO_PORT=1001 expo run:ios",
    "dev:android": "RCT_METRO_PORT=1001 yarn prebuild --platform android --clean && yarn android --no-build-cache --port 1001",
    "dev:ios": "RCT_METRO_PORT=1001 yarn prebuild --platform ios --clean && yarn ios --no-build-cache --port 1001",
    "deploy:ios": "eas build --platform ios --profile production && eas submit --platform ios",
    "deploy:android": "eas build --platform android --profile production && eas submit --platform android",
    "export:web": "expo export --platform web",
    "deploy:web": "PORT=1001 serve -s dist",
    "lint": "expo lint"
  },
  "dependencies": {
    "@apollo/client": "^4.0.7",
    "@apollo/react-components": "^4.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-voice/voice": "^3.2.4",
    "@shopify/react-native-skia": "2.2.12",
    "@siteed/expo-audio-studio": "^2.18.1",
    "@tanstack/react-query": "^5.89.0",
    "axios": "^1.12.2",
    "buffer": "^6.0.3",
    "connect": "^3.7.0",
    "date-fns": "^4.1.0",
    "expo": "^54.0.0",
    "expo-audio": "^1.0.13",
    "expo-av": "^16.0.7",
    "expo-camera": "^17.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "^6.0.12",
    "expo-file-system": "^19.0.16",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-linking": "~8.0.8",
    "expo-location": "^19.0.7",
    "expo-notifications": "~0.32.11",
    "expo-router": "~6.0.6",
    "expo-secure-store": "^15.0.7",
    "expo-speech": "^14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.0.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.6.0",
    "rxjs": "^7.8.2",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "http-proxy-middleware": "^3.0.5",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2"
  }
}
